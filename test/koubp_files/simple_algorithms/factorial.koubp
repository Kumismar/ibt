function factorial(int number): int {
    if (number < 0) {
        return -1;
    }
    elseif (number <= 1) {
        return 1;
    }

    return number * factorial(number - 1);
}

print("Give a number to calculate factorial:\n");
int number = convert_int(readline());
print("result is:" + convert_string(factorial(number)));